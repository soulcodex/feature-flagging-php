{
  "name": "soulcodex/feature-flags-for-all",
  "description": "A basic feature flagging implementation in PHP",
  "type": "project",
  "require": {
    "php": "^8.2",
    "ext-ctype": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "slim/slim": "4.*",
    "slim/psr7": "^1.6",
    "predis/predis": "^2.2",
    "ramsey/uuid": "^4.7",
    "lambdish/phunctional": "^2.1",
    "php-di/slim-bridge": "^3.4",
    "not-empty/ulid-php-lib": "6.0",
    "flagsmith/flagsmith-php-client": "^4.2",
    "symfony/http-client": "^7.0",
    "nyholm/psr7": "^1.8",
    "symfony/cache": "^7.0",
    "psr/http-client": "^1.0",
    "unleash/client": "^2.3",
    "monolog/monolog": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Soulcodex\\App\\": "src",
      "Soulcodex\\App\\DependencyInjection\\": "di"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Soulcodex\\Test\\": [
        "tests"
      ]
    }
  },
  "license": "propietary",
  "authors": [
    {
      "name": "Roberto Garcia"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
